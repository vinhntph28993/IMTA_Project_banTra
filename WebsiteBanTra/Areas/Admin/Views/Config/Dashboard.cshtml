        <a href="~/Admin/Config/Create" class="text-white"><i class="fas fa-edit fa-xs"></i>&nbsp;Cấu hình mới cho Website</a>
    </button>
}
else

{
    <p style="color: #ebebeb ">\\\\\\\</p>
    <button type="button" class="btn btn-success btn-sm">
        <a href="~/" target="_blank" class="text-white"><i class="fas fa-home fa-xs"></i>&nbsp;@ViewBag.Configs.Name</a>
    </button>

    <p style="color: #ebebeb ">\\\\\\\</p>
    <button type="button" class="btn btn-secondary btn-sm">
        <a href="~/Admin/Config/Edit/1" class="text-white"><i class="fas fa-edit fa-xs"></i>&nbsp;Chỉnh sửa cấu hình cho Website</a>
    </button>
